{
  "name": "package-b",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "yarn gatsby build",
    "develop": "yarn gatsby develop",
    "start": "yarn gatsby develop",
    "serve": "yarn gatsby serve",
    "clean": "yarn gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "yarn eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "yarn prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,css}\""
  },
  "dependencies": {
    "@shared/component-library": "1.0.0",
    "gatsby": "^4.21.1",
    "gatsby-plugin-typescript": "^4.21.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "eslint": "^8.23.0",
    "prettier": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
